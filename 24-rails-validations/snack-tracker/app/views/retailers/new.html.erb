<h1>Create a new Retailer</h1>

<%= render partial: "shared/error_list", locals: {new_object: @retailer} %>

<%= form_for @retailer do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :year_established %>
    <%= f.number_field :year_established %>
    <%= f.submit %>
<% end %>